{
  "$schema": "https://turborepo.com/schema.json",
  "tasks": {
    "test": {
      "cache": false,
      "outputs": []
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**",
        "lib/**"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "lint": {
      "cache": false
    },
    "lint:unsafe": {
      "cache": false
    },
    "lint:write": {
      "cache": false
    },
    "db:push": {
      "cache": false
    },
    "db:push:sqlite": {
      "cache": false
    },
    "db:push:pg": {
      "cache": false
    },
    "db:studio": {
      "cache": false
    },
    "db:studio:sqlite": {
      "cache": false
    },
    "db:studio:pg": {
      "cache": false
    }
  }
}